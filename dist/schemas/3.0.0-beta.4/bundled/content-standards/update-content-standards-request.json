                        "type": "object",
                        "description": "Bearer token authentication",
                        "properties": {
                          "method": {
                            "type": "string",
                            "const": "bearer_token"
                          },
                          "token": {
                            "type": "string",
                            "description": "OAuth2 bearer token for Authorization header"
                          }
